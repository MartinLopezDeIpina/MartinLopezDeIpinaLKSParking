<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextInputStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeWidthFocused">4dp</item>
    </style>
    <style name="registerTextInput" parent="TextInputStyle">
    </style>
    <style name="constraintHeader">
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="android:textSize">36sp</item>
        <item name="fontFamily">@font/robotobold</item>
        <item name="android:textAlignment">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="HeadingStyleConstrainted">
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="android:textSize">36sp</item>
        <item name="fontFamily">@font/robotobold</item>
        <item name="android:textAlignment">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="HeadingStyle">
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="android:textSize">36sp</item>
        <item name="fontFamily">@font/robotobold</item>
        <item name="android:textAlignment">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">36dp</item>
    </style>
    <style name="HeadingFlechaStyle" parent="HeadingStyle">
        <item name="android:layout_marginTop">0dp</item>
    </style>
    <style name="HeadingSecondaryStyle">
        <item name="android:layout_marginTop">50dp</item>
        <item name="android:layout_marginBottom">25dp</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="android:textSize">22sp</item>
        <item name="fontFamily">@font/robotoregular</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="HeadingSecondaryStyleAddBooking" parent="HeadingSecondaryStyle">
        <item name="android:layout_marginTop">25dp</item>
    </style>
    <style name="LoginHeadingStyle" parent="HeadingStyle">
        <item name="android:layout_marginTop">48dp</item>
        <item name="android:layout_marginStart">108dp</item>
    </style>
    <style name="PrimaryButton">
        <item name="fontFamily">@font/robotobold</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="backgroundTint">?attr/colorPrimaryContainer</item>
        <item name="textFillColor">?attr/colorOnPrimary</item>
        <item name="android:textSize">18sp</item>
        <item name="android:paddingLeft">28dp</item>
        <item name="android:paddingTop">14dp</item>
        <item name="android:paddingRight">28dp</item>
        <item name="android:paddingBottom">14dp</item>
        <item name="android:layout_width">250dp</item>
    </style>
    <style name="NavStyleUnselected" parent="TextAppearance.AppCompat.Body1">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/robotomedium</item>
    </style>
    <style name="NavStyleSelected" parent="TextAppearance.AppCompat.Body1">
        <item name="android:textSize">15sp</item>
        <item name="android:fontFamily">@font/robotobold</item>
    </style>
    <style name="ProfileInfoHeader">
        <item name="android:textColor">?attr/colorSurfaceVariant</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="ProfileInfo">
        <item name="android:textColor">?attr/colorOnSurfaceVariant</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="baseLayout">
        <item name="android:layout_marginStart">20dp</item>
    </style>
    <style name="registerInputSeparation" parent="TextInputStyle">
        <item name="android:layout_marginTop">20dp</item>
    </style>
    <style name="sortingChip" parent="Widget.MaterialComponents.Chip.Filter">
        <item name="checkedIcon">@null</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:textColor">?attr/colorOnSecondaryContainer</item>
        <item name="android:background">?attr/colorSecondaryContainer</item>
        <item name="android:layout_marginStart">7dp</item>
        <item name="android:layout_marginEnd">7dp</item>
    </style>
    <style name="bookingChip" parent="Widget.MaterialComponents.Chip.Filter">
        <item name="android:textSize">14sp</item>
        <item name="android:layout_height">70dp</item>
        <item name="checkedIcon">@null</item>
    </style>
    <style name="dateChip" parent="Widget.MaterialComponents.Chip.Filter">
        <item name="checkedIcon">@null</item>
    </style>
    <style name="hourChip" parent="Widget.MaterialComponents.Chip.Filter">
        <item name="checkedIcon">@null</item>
    </style>
    <style name="plazaChip" parent="Widget.MaterialComponents.Chip.Filter">
        <item name="checkedIcon">@null</item>
    </style>
    <style name="diaSemana">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">?attr/colorOnPrimaryContainer</item>
        <item name="android:layout_marginStart">4dp</item>
        <item name="android:layout_marginEnd">4dp</item>
        <item name="android:fontFamily">@font/robotomedium</item>
        <item name="android:alpha">0.2</item>
    </style>
    <style name="iconoAtrasConstrainted">
        <item name="android:tint">?attr/colorOnSurfaceVariant</item>
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_marginStart">30dp</item>
    </style>
    <style name="iconoAtras">
        <item name="android:tint">?attr/colorOnSurfaceVariant</item>
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_marginStart">30dp</item>
        <item name="android:layout_marginTop">30dp</item>
    </style>
    <style name="iconoDesactivado">
        <item name="android:color">?attr/colorSurfaceInverse</item>
        <item name="android:tint">?attr/colorSurfaceInverse</item>
    </style>
</resources>